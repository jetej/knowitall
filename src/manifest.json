{
  "manifest_version": 2,

  "description": "Chrome extension news feed",
  "name": "knowitall",
  "version": "1.0",
  "minimum_chrome_version": "20",

  "chrome_url_overrides" : {
    "newtab": "start.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "webRequest",
    "webNavigation"
  ]
}
