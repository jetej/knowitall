{
  "manifest_version": 2,

  "description": "Chrome extension news feed",
  "name": "knowitall",
  "version": "1.0",
  "minimum_chrome_version": "20",

  "chrome_url_overrides" : {
    "newtab": "home.html"
  },
  "browser_action": {
    "default_icon": "flow.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "webRequest",
    "webNavigation"
  ]
}
